graph001
graph(G)
ascii:@G@ is a connected graph
graph(a[]).
graph(ab[ab]).
graph(abc[ab;ac;bc]).

graph(abcdefg[ab;bc;cd;da;ae;bf;cg;dh;ef;fg;gh;he]).
graph(abcdefg[ab;bc;cd;da;ae;bf;cg;dh;ef;fg;gh;he]).

graph002
node(G,N)
ascii:@N@ is a node of @G@
node(G,N) -> graph(G)
Code:

graph003
edge(G,E)
ascii:@E@ is an edge of @G@
edge(G,E) -> graph(G)
Code:

graph004
node_on_edge(G,N,E)
ascii:@N@ is on @E@ in @G@
node_on_edge(G,N,E) -> graph(G)
node_on_edge(G,N,E) -> node(G,N)
node_on_edge(G,N,E) -> edge(G,E)
Code:

graph005
